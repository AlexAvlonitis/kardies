.row
  .col-lg-6.col-md-12.col-sm-12.bg-white.effect2.m-3.p-4
    %h1.strong.orange-text
      = t '.my_hearts'
    %hr
    .row
      - @likes.each do |user|
        = link_to user_path(user) do
          .profile-pic-round-sm.m-2.lazy{ "data-src" => "#{user.profile_picture}" }

  .col-lg-5.col-md-12.col-sm-12.bg-white.effect2.m-3.p-4
    = render partial: 'suggested_people/list',
      locals: { users: @suggested_users }

.row
  .col
    .kardies-ad
      = render partial: 'layouts/kardies_ad'
